def check_credentials():
    username = input("กรุณากรอก Username: ")
    password = input("กรุณากรอก Password: ")
    
    if username == "admin" and password == "Ad13@23t":
        print("ยินดีต้อนรับเข้าสู่ระบบ Admin")
    else:
        print("Username หรือ Password ไม่ถูกต้อง")

check_credentials()
